package kr.board.entity;

import lombok.AllArgsConstructor;
import lombok.Data;

@Data
@AllArgsConstructor
public class Board {
	private int idx; // 번호
	private String title; // 제목
	private String contents; // 내용
	private String Writer; // 작성자
	private String indate; // 작성일
	private int count; // 조회수
	

}
